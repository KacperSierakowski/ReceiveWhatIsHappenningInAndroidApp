<Window x:Class="ReceiveWhatIsHappenningInAndroidApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReceiveWhatIsHappenningInAndroidApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="ResponseImage" HorizontalAlignment="Left" Height="334" Margin="50,24,0,0" VerticalAlignment="Top" Width="224"/>
        <Button Content="START" HorizontalAlignment="Left" Margin="50,389,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <RichTextBox x:Name="richTextBox" HorizontalAlignment="Left" Height="334" Margin="563,24,0,0" VerticalAlignment="Top" Width="203">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
